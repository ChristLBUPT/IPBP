#!/bin/bash
model_type="OpenELM-3B"
lora_config_str='{"target_modules": ["qkv_proj"]}'
cmd_args="--do_train 
--train_data_path=./data/train.conllu 
--dev_data_path=./data/dev.conllu 
--use_llm 
--lora_config='${lora_config_str}'
--bert_model=../pretrained-models/${model_type}
--n_mlp_arc=500 
--n_mlp_rel=100 
--max_seq_length=300 
--train_batch_size=16
--eval_batch_size=16
--gas=16
--use_${external} 
--num_train_epochs=${num_epochs} 
--warmup_proportion=0.1 
--learning_rate=3e-5 
--num_workers=0
--patient=100"